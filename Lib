			Box_2.AnchorPoint = Vector2.new(0, 0.5)
			Box_2.BackgroundColor3 = Color3.fromRGB(61, 58, 61)
			Box_2.BorderSizePixel = 0
			Box_2.Position = UDim2.new(0.75, 0, 0.5, 0)
			Box_2.Selectable = false
			Box_2.Size = UDim2.new(0, 100, 1, 0)
			Box_2.PlaceholderText = ""
			Box_2.Text = options.default
			Box_2.TextColor3 = Color3.fromRGB(255, 255, 255)
			Box_2.TextSize = 6.000
			Box_2.TextWrapped = true
			local bround4 = Instance.new("UICorner",Box_2)bround4.CornerRadius = Vector2(0,6)
			Box_2.FocusLost:connect(function(e)
				local old = location[flag];
				if type == "number" then
					local num = tonumber(Box_2.Text)
					if (not num) then
						Box_2.Text = tonumber(location[flag])
					else
						location[flag] = math.clamp(num, min, max)
						Box_2.Text = tonumber(location[flag])
					end
				else
					location[flag] = tostring(Box_2.Text)
				end

				callback(location[flag], old, e)
			end)

			if type == 'number' then
				Box_2:GetPropertyChangedSignal('Text'):connect(function()
					Box_2.Text = string.gsub(Box_2.Text, "[%a+]", "");
				end)
			end
			Section.Size = Section.Size + UDim2.new(0,0,0,30)
			Page_2.CanvasSize = Page_2.CanvasSize + UDim2.new(0,0,0,15)
		end
