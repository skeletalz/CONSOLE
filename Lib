local function TORSJLL_fake_script() -- C0NLE.Hide/show 
	local script = Instance.new('LocalScript', C0NLE)

	local UIS = game:GetService("UserInputService")
	local shown = true;
	local function tween(ob,args,props)
		game:GetService("TweenService"):Create(ob,TweenInfo.new(args),props):Play()
	end
	UIS.InputBegan:Connect(function(key) 
		if key.KeyCode == _G.Opencode then
			if shown == true then
				tween(script.Parent.Main,0.4,{Size = UDim2.new(0,675,0,16)})wait(0.3)
				tween(script.Parent.Main.Top,0.3,{Size = UDim2.new(0,90,0,18)})script.Parent.Main.Top.Bind.Visible=false;tween(script.Parent.Main,0.3,{Size = UDim2.new(0,90,0,16)})wait(0.3)script.Parent.Main.Top.Fix.Visible = false;wait(0.1)
				shown = false
			else
				tween(script.Parent.Main,0.3,{Size = UDim2.new(0,675,0,16)})
				tween(script.Parent.Main.Top,0.3,{Size = UDim2.new(0,675,0,19)})script.Parent.Main.Top.Bind.Visible=true;script.Parent.Main.Top.Fix.Visible = true;wait(0.3);tween(script.Parent.Main,0.3,{Size = UDim2.new(0,675,0,353)})wait(0.1)
				shown = true
			end
		end
	end)
	spawn(function()
		while wait() do
			if shown == false then
				tween(script.Parent.Main.Top.Title,0.8,{TextColor3 = Color3.fromRGB(255,255,255)})wait(1.2)
				tween(script.Parent.Main.Top.Title,0.8,{TextColor3 = Color3.fromRGB(114, 118, 125)})wait(0.8)
			end
		end
	end)
end
coroutine.wrap(TORSJLL_fake_script)()
function library:SetActive(page)
	local function tween(ob,args,props)
		game:GetService("TweenService"):Create(ob,TweenInfo.new(args),props):Play()
	end
	for i,v in pairs(Pages:GetChildren()) do
		v.Visible = false
	end
	for p,g in pairs(Menu:GetChildren()) do
		if g:IsA("TextButton") then tween(g,0.3,{TextColor3 = Color3.fromRGB(99, 104, 114)})end
	end
	print(page)
	Main.Pages[page].Visible = true
	tween(Menu[page],0.3,{TextColor3 = Color3.fromRGB(255,255,255)})wait(0.3)
end
function library:Reload()
	local function tween(ob,args,props)
		game:GetService("TweenService"):Create(ob,TweenInfo.new(args),props):Play()
	end
	for i,v in pairs(Menu:GetChildren()) do
		for x,d in pairs(Pages:GetChildren()) do
			if d.Visible == true and v.Name == d.Name then tween(v,0.3,{TextColor3 = Color3.fromRGB(255,255,255)})wait(0.3) end 
			if v.Name == d.Name then
				v.MouseEnter:Connect(function()
					if d.Visible == false then
					tween(v,0.3,{TextColor3 = Color3.fromRGB(255,255,255)})wait(0.3) end
				end)
				v.MouseLeave:Connect(function()
					if d.Visible == false then
					tween(v,0.3,{TextColor3 = Color3.fromRGB(99, 104, 114)})wait(0.3) end
				end)
				v.MouseButton1Click:Connect(function()
					for p,g in pairs(Pages:GetChildren()) do
						g.Visible = false
					end
					for p,g in pairs(Menu:GetChildren()) do
						if g:IsA("TextButton")and g~=v then tween(g,0.3,{TextColor3 = Color3.fromRGB(99, 104, 114)})end
					end
					d.Visible = true
				end)
			end
		end
	end
end
spawn(function()
		while wait() do
		if C0NLE:FindFirstChild("Watermark") == nil then
				game.Players.LocalPlayer:Kick("Kicked by C0N$ØLE \n NO WATERMARK = NO UI")
			end
		if C0NLE:FindFirstChild("Watermark").Visible == false then
				game.Players.LocalPlayer:Kick("Kicked by C0N$ØLE \n NO WATERMARK = NO UI")
			end
		if C0NLE:FindFirstChild("Watermark").Position ~= UDim2.new(0.00528455153, 0, -0.000239789762, 0) then
				game.Players.LocalPlayer:Kick("Kicked by C0N$ØLE \n NO WATERMARK = NO UI")
			end
		if C0NLE:FindFirstChild("Watermark").Size ~= UDim2.new(0, 434, 0, 55) then
				game.Players.LocalPlayer:Kick("Kicked by C0N$ØLE \n NO WATERMARK = NO UI")
			end
			if C0NLE:FindFirstChild("Watermark").TextSize ~= 19.000 then
				game.Players.LocalPlayer:Kick("Kicked by C0N$ØLE \n NO WATERMARK = NO UI")
			end
		end
	C0NLE:FindFirstChild("Watermark"):GetPropertyChangedSignal("Text"):Connect(function()
		C0NLE:FindFirstChild("Watermark").Text = "(C0N$ØLE) by TheNonGamerIsHere#4698 / SlaxxF#0001"
	end)
end)
local function HPAFQ_fake_script() -- C0NLE.Drag 
	local script = Instance.new('LocalScript', C0NLE)

	local dragger = {}; 
		local mouse        = game:GetService("Players").LocalPlayer:GetMouse();
		local inputService = game:GetService('UserInputService');
		local heartbeat    = game:GetService("RunService").Heartbeat;
		-- // credits to Ririchi / Inori for this cute drag function :)
		function dragger.new(frame)
			local s, event = pcall(function()
				return frame.MouseEnter
			end)
	
			if s then
				frame.Active = true;
	
				event:connect(function()
					local input = frame.InputBegan:connect(function(key)
						if key.UserInputType == Enum.UserInputType.MouseButton1 then
							local objectPosition = Vector2.new(mouse.X - frame.AbsolutePosition.X, mouse.Y - frame.AbsolutePosition.Y);
							while heartbeat:wait() and inputService:IsMouseButtonPressed(Enum.UserInputType.MouseButton1) do
								pcall(function()
									frame:TweenPosition(UDim2.new(0, mouse.X - objectPosition.X, 0, mouse.Y - objectPosition.Y), 'Out', 'Linear', 0.1, true);
								end)
							end
						end
					end)
	
					local leave;
					leave = frame.MouseLeave:connect(function()
						input:disconnect();
						leave:disconnect();
					end)
				end)
			end
		end
		dragger.new(script.Parent.Main)
end
coroutine.wrap(HPAFQ_fake_script)()
return library;
