			Page_2.CanvasSize = Page_2.CanvasSize + UDim2.new(0,0,0,15)
		end
		function sect:Toggle(name,options,callback)
			local default  = options.default or false;
			local location = options.location or tab.flags;
			local flag     = options.flag or "";
			local callback = callback or function() end;

			location[flag] = default;
			local Toggle = Instance.new("TextButton")
			local Slidertext = Instance.new("TextLabel")
			local ToggleBTN = Instance.new("TextButton")
			local ToggleBTNiner = Instance.new("TextButton")
			Toggle.Name = name
			Toggle.Parent = Section
			Toggle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			Toggle.BackgroundTransparency = 1.000
			Toggle.BorderSizePixel = 0
			Toggle.Size = UDim2.new(1, 0, 0, 24)
			Toggle.AutoButtonColor = false
			Toggle.Font = Enum.Font.SourceSans
			Toggle.Text = ""
			Toggle.TextColor3 = Color3.fromRGB(0, 0, 0)
			Toggle.TextSize = 14.000

			Slidertext.Name = "Toggle text"
			Slidertext.Parent = Toggle
			Slidertext.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			Slidertext.BackgroundTransparency = 1.000
			Slidertext.BorderSizePixel = 0
			Slidertext.Size = UDim2.new(0, 200, 0, 18)
			Slidertext.ZIndex = 3
			Slidertext.Font = Enum.Font.Highway
			Slidertext.Text = name
			Slidertext.TextColor3 = Color3.fromRGB(214, 222, 234)
			Slidertext.TextSize = 12.000
			Slidertext.TextXAlignment = Enum.TextXAlignment.Left

			ToggleBTN.Name = "ToggleBTN"
			ToggleBTN.Parent = Toggle
			ToggleBTN.AnchorPoint = Vector2.new(0, 0.5)
			ToggleBTN.BackgroundColor3 = Color3.fromRGB(61, 58, 61)
			ToggleBTN.BorderSizePixel = 0
			ToggleBTN.Position = UDim2.new(0.887000024, 0, 0.5, 0)
			ToggleBTN.Size = UDim2.new(0, 24, 0, 24)
			ToggleBTN.AutoButtonColor = false
			ToggleBTN.Font = Enum.Font.Highway
			ToggleBTN.Text = ""
			ToggleBTN.TextColor3 = Color3.fromRGB(255, 0, 0)
			ToggleBTN.TextSize = 16.000
			local bround2 = Instance.new("UICorner",ToggleBTN)bround2.CornerRadius = Vector2(0,6)
			ToggleBTNiner.Name = "ToggleBTNiner"
			ToggleBTNiner.Parent = ToggleBTN
			ToggleBTNiner.AnchorPoint = Vector2.new(0.5, 0.5)
			ToggleBTNiner.BackgroundColor3 = Color3.fromRGB(35, 43, 70)
			ToggleBTNiner.BorderSizePixel = 0
			ToggleBTNiner.Position = UDim2.new(0.5, 0, 0.5, 0)
			ToggleBTNiner.Size = UDim2.new(1, -4, 1, -4)
			ToggleBTNiner.AutoButtonColor = false
			ToggleBTNiner.Font = Enum.Font.Highway
			ToggleBTNiner.Text = ""
			ToggleBTNiner.TextColor3 = Color3.fromRGB(255, 0, 0)
			ToggleBTNiner.TextSize = 16.000
			local bround3 = Instance.new("UICorner",ToggleBTNiner)bround3.CornerRadius = Vector2(0,6)
			local function click(t)
				location[flag] = not location[flag];
				callback(location[flag])
			end
			local tog = false
			local function tween(args)
				game:GetService("TweenService"):Create(args.object,TweenInfo.new(args.info),args.props):Play()
			end
			if default == true then
				tog = false
				tween({object=ToggleBTN,info=0.3,props={BackgroundColor3 = Color3.fromRGB(122, 116, 122)}})
				tween({object=ToggleBTNiner,info=0.3,props={BackgroundColor3 = Color3.fromRGB(61, 58, 61)}})
			end
			local function toggle()
				if tog then tog = false
					tween({object=ToggleBTN,info=0.3,props={BackgroundColor3 = Color3.fromRGB(122, 116, 122)}})
					tween({object=ToggleBTNiner,info=0.3,props={BackgroundColor3 = Color3.fromRGB(61, 58, 61)}})
				else
					tog = true
					tween({object=ToggleBTN,info=0.3,props={BackgroundColor3 = Color3.fromRGB(61, 58, 61)}})
					tween({object=ToggleBTNiner,info=0.3,props={BackgroundColor3 = Color3.fromRGB(35, 43, 70)}})
				end
			end
			Toggle.MouseButton1Click:Connect(function()
				toggle()
				click()
			end)
			ToggleBTN.MouseButton1Click:Connect(function()
				toggle()
				click()
			end)
			ToggleBTNiner.MouseButton1Click:Connect(function()
				toggle()
				click()
			end)
			library.callbacks[flag] = click;

			if location[flag] == true then
				callback(location[flag])
			end
			Section.Size = Section.Size + UDim2.new(0,0,0,30)
			Page_2.CanvasSize = Page_2.CanvasSize + UDim2.new(0,0,0,15)
		end
