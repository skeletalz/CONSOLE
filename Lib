				tween({object=effect_2,info=0.3,props={Size=UDim2.new(1,8,1,8)}})wait(0.2)tween({object=effect_2,info=0.1,props={Size=UDim2.new(1,-4,1,-4)}})wait(0.1)effect_2.Size = UDim2.new(1,-8,1,-8)
			end)
			Section.Size = Section.Size + UDim2.new(0,0,0,30)
			Page_2.CanvasSize = Page_2.CanvasSize + UDim2.new(0,0,0,15)
		end
