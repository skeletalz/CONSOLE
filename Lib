			local down = false
			if default ~= min then
				game:GetService("TweenService"):Create(Inner, TweenInfo.new(0.05), {Size = UDim2.new(default/max, 0, 0, 8)}):Play()
				Inner.Text = default
			end
			local function Activate()
				spawn(function()
					down = true
					Value = math.floor((((tonumber(max) - tonumber(min)) / 110) * Inner.AbsoluteSize.X) +tonumber(min)) or 0
					pcall(function()
						callback(tonumber(Value))
						location[flag] = tonumber(Value)
					end)
					Number.Text = Value     
					game:GetService("TweenService"):Create(Inner, TweenInfo.new(0.05), {Size = UDim2.new(0, math.clamp(mouse.X - Inner.AbsolutePosition.X, 0, 110), 0, 8)}):Play()
					game:GetService("TweenService"):Create(Inner, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(113, 117, 124)}):Play()
					while game:GetService("RunService").RenderStepped:wait() and down do
						Value = math.floor((((tonumber(max) - tonumber(min)) / 110) * Inner.AbsoluteSize.X) +tonumber(min)) or 0
						Number.Text = Value               
						pcall(function()
							callback(tonumber(Value))
							location[flag] = tonumber(Value)
						end)
