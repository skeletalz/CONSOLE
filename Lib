		end
		function sect:Bind(name, options, callback)
			local location     = options.location or tab.flags;
			local keyboardOnly = options.kbonly or false
			local flag         = options.flag or "";
			local callback     = callback or function() end;
			local default      = options.default;
			local Bind_3 = Instance.new("TextButton")
			local Bindtext_2 = Instance.new("TextLabel")
			local Bind_4 = Instance.new("TextButton")
			Bind_3.Name = name
			Bind_3.Parent = Section
			Bind_3.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			Bind_3.BackgroundTransparency = 1.000
			Bind_3.BorderSizePixel = 0
			Bind_3.Size = UDim2.new(1, 0, 0, 24)
			Bind_3.AutoButtonColor = false
			Bind_3.Font = Enum.Font.SourceSans
			Bind_3.Text = ""
			Bind_3.TextColor3 = Color3.fromRGB(0, 0, 0)
			Bind_3.TextSize = 14.000

			Bindtext_2.Name = "Bind text"
			Bindtext_2.Parent = Bind_3
			Bindtext_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			Bindtext_2.BackgroundTransparency = 1.000
			Bindtext_2.BorderSizePixel = 0
			Bindtext_2.Size = UDim2.new(0, 200, 1, 0)
			Bindtext_2.ZIndex = 3
			Bindtext_2.Font = Enum.Font.Highway
			Bindtext_2.Text = name
			Bindtext_2.TextColor3 = Color3.fromRGB(214, 222, 234)
			Bindtext_2.TextSize = 12.000
			Bindtext_2.TextXAlignment = Enum.TextXAlignment.Left

			Bind_4.Name = "Bind"
			Bind_4.Parent = Bindtext_2
			Bind_4.Active = false
			Bind_4.AnchorPoint = Vector2.new(0, 0.5)
			Bind_4.BackgroundColor3 = Color3.fromRGB(61, 58, 61)
			Bind_4.BorderSizePixel = 0
			Bind_4.Position = UDim2.new(0.75, 0, 0.5, 0)
			Bind_4.Selectable = false
			Bind_4.Size = UDim2.new(0, 100, 1, 0)
			Bind_4.AutoButtonColor = false
			Bind_4.Text = ""
			Bind_4.TextColor3 = Color3.fromRGB(255, 255, 255)
			Bind_4.TextSize = 6.000
			Bind_4.TextWrapped = true
			local bround9 = Instance.new("UICorner",Bind_4)bround9.CornerRadius = Vector2(0,6)
			local binding = false
			local banned = {
				Return = true;
				Space = true;
				Tab = true;
				Unknown = true;
			}

			local shortNames = {
				RightControl = 'RightCtrl';
				LeftControl = 'LeftCtrl';
				LeftShift = 'LShift';
				RightShift = 'RShift';
				MouseButton1 = "LMB";
				MouseButton2 = "RMB";
			}

			local allowed = {
				MouseButton1 = true;
				MouseButton2 = true;
			}      

			local nm = (default and (shortNames[default.Name] or default.Name) or "None");
			Bind_3.MouseButton1Click:connect(function()
				binding = true

				Bind_4.Text = "..."
				local a, b = game:GetService('UserInputService').InputBegan:wait();
				local name = tostring(a.KeyCode.Name);
				local typeName = tostring(a.UserInputType.Name);

				if (a.UserInputType ~= Enum.UserInputType.Keyboard and (allowed[a.UserInputType.Name]) and (not keyboardOnly)) or (a.KeyCode and (not banned[a.KeyCode.Name])) then
					local name = (a.UserInputType ~= Enum.UserInputType.Keyboard and a.UserInputType.Name or a.KeyCode.Name);
					Bind_4.Text = shortNames[name] or name;
					location[flag] = (a);

				end
				wait(0.1)  
				binding = false;
			end)
			Bind_4.MouseButton1Click:Connect(function()
				binding = true

				Bind_4.Text = "..."
				local a, b = game:GetService('UserInputService').InputBegan:wait();
				local name = tostring(a.KeyCode.Name);
				local typeName = tostring(a.UserInputType.Name);

				if (a.UserInputType ~= Enum.UserInputType.Keyboard and (allowed[a.UserInputType.Name]) and (not keyboardOnly)) or (a.KeyCode and (not banned[a.KeyCode.Name])) then
					local name = (a.UserInputType ~= Enum.UserInputType.Keyboard and a.UserInputType.Name or a.KeyCode.Name);
					Bind_4.Text = shortNames[name] or name;
					location[flag] = (a);

				end
				wait(0.1)  
				binding = false;
			end)
			Section.Size = Section.Size + UDim2.new(0,0,0,30)
			Page_2.CanvasSize = Page_2.CanvasSize + UDim2.new(0,0,0,15)
		end
		return sect;
	end
	return tab;
end
local function TLRXB_fake_script() -- Bind.LocalScript 
	local script = Instance.new('LocalScript', Bind)

	local options = {}
	local keyboardOnly = options.kbonly or true
	local flag         = options.flag or "";
	local default      = Enum.KeyCode.RightControl
	_G.Opencode=Enum.KeyCode.RightControl
	local binding = false
	local banned = {
		Return = true;
		Space = true;
		Tab = true;
		Unknown = true;
	}
	
	local shortNames = {
		RightControl = 'RightCtrl';
		LeftControl = 'LeftCtrl';
		LeftShift = 'LShift';
		RightShift = 'RShift';
		MouseButton1 = "LMB";
		MouseButton2 = "RMB";
	}
	
	local allowed = {
		MouseButton1 = true;
		MouseButton2 = true;
	}      
	
	local nm = (default and (shortNames[default.Name] or default.Name) or "None");
	script.Parent.MouseButton1Click:connect(function()
		binding = true
	
		script.Parent["Bind text"].Bind.Text = "..."
		local a, b = game:GetService('UserInputService').InputBegan:wait();
		local name = tostring(a.KeyCode.Name);
		local typeName = tostring(a.UserInputType.Name);
	
		if (a.UserInputType ~= Enum.UserInputType.Keyboard and (allowed[a.UserInputType.Name]) and (not keyboardOnly)) or (a.KeyCode and (not banned[a.KeyCode.Name])) then
			local name = (a.UserInputType ~= Enum.UserInputType.Keyboard and a.UserInputType.Name or a.KeyCode.Name);
				script.Parent["Bind text"].Bind.Text = shortNames[name] or name;
			
			
		end
		wait(0.1)  
		binding = false;
	end)
	script.Parent["Bind text"].Bind.MouseButton1Click:Connect(function()
		binding = true
	
		script.Parent["Bind text"].Bind.Text = "..."
		local a, b = game:GetService('UserInputService').InputBegan:wait();
		local name = tostring(a.KeyCode.Name);
		local typeName = tostring(a.UserInputType.Name);
	
		if (a.UserInputType ~= Enum.UserInputType.Keyboard and (allowed[a.UserInputType.Name]) and (not keyboardOnly)) or (a.KeyCode and (not banned[a.KeyCode.Name])) then
			local name = (a.UserInputType ~= Enum.UserInputType.Keyboard and a.UserInputType.Name or a.KeyCode.Name);
			script.Parent["Bind text"].Bind.Text = shortNames[name] or name;
			_G.Opencode=a.KeyCode
	
		end
		wait(0.1)  
		binding = false;
	end)
end
